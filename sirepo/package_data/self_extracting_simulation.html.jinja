<html>
<head>
<meta http-equiv="Content-Type" content="{{ mimetype }}; charset={{ charset }}" />
<link rel="icon" type="image/png" href="data:image/png;base64," />
<title>{{ appShortName | e }} - {{ productShortName | e }}</title>
<style type="text/css">
body {
margin: 0;
padding: 0;
font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.sr_ses_head {
background-color: rgb(248, 248, 248);
color: rgb(51, 122, 183);
font-size: 18px;
vertical-align: middle;
border-color: rgb(231, 231, 231);
border-bottom-width: 1px;
border-style: solid;
}
.sr_ses_head img {
margin: 5px 10px 5px 30px;
height: 40px;
vertical-align: middle;
}
.sr_ses_main {
margin: 20px 10px 10px 30px;
}
.sr_ses_prompt {
display: block;
margin-bottom: 10px;
}
.sr_ses_server {
width: 30em;
margin-right: 5px;
}
</style>
<script type="text/javascript">
var SIREPO=(function() {
return {
onSubmit: function (form) {
form.action = form.elements['server'].value + "{{ uri }}";
return true;
}
};
})();
</script>
</head>
<body>
<div class="sr_ses_head">
<img src="data:image/gif;base64," />
<span>{{ appLongName | e }} - {{ productLongName | e }}</div>
<div class="sr_ses_main"><span class="sr_ses_prompt">Self-Extracting Simulation: {{ data.models.simulation.name | e }}</span>
<form onsubmit="SIREPO.onSubmit(this)" action="" method="post">
<input class="sr_ses_server" name="server" type="text" value="{{ server }}"/>
<button type="submit">Send to Server</button>
<input name="zip" type="hidden" value="{{ zip_tag }}" />
</form>
</div>
</body>
</html>
